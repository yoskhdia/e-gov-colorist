{
    "name": "e-gov-colorist",
    "version": "1.0.0",
    "description": "e-Gov法令検索サイト上で使用できる、いくつかの補助機能を追加します。",
    "engines": {
        "yarn": "^1.22.19"
    },
    "private": true,
    "scripts": {
        "build": "webpack --config webpack.prod.js",
        "build-firefox": "webpack --config webpack.prod.js --env APP_TARGET=firefox",
        "dev": "webpack -w --config webpack.dev.js",
        "dev-firefox": "webpack -w --config webpack.dev.js --env APP_TARGET=firefox",
        "test": "jest --config=jest.config.js",
        "lint": "eslint --fix -c ./.eslintrc.js \"src/**/*.ts*\"",
        "prettify": "prettier --write \"src/**/*.ts*\"",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yoskhdia/e-gov-colorist.git"
    },
    "keywords": [
        "e-gov",
        "firefox",
        "chrome",
        "extension"
    ],
    "author": "Yoshitaka Okuda",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/preset-env": "^7.19.3",
        "@babel/preset-typescript": "^7.18.6",
        "@storybook/addon-essentials": "^6.5.12",
        "@storybook/addon-interactions": "^6.5.12",
        "@storybook/builder-webpack5": "^6.5.12",
        "@storybook/manager-webpack5": "^6.5.12",
        "@storybook/react": "^6.5.12",
        "@storybook/testing-library": "^0.0.13",
        "@types/chrome": "^0.0.197",
        "@types/jest": "^29.1.2",
        "@types/node": "^18.8.3",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "@types/react-test-renderer": "^18.0.0",
        "@types/webextension-polyfill": "^0.9.1",
        "@typescript-eslint/eslint-plugin": "^5.39.0",
        "@typescript-eslint/parser": "^5.39.0",
        "autoprefixer": "^10.4.12",
        "babel-core": "^6.26.3",
        "babel-jest": "^29.1.2",
        "babel-loader": "^8.2.5",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.8",
        "jest": "^29.1.2",
        "jest-css-modules": "^2.1.0",
        "postcss": "^8.4.17",
        "postcss-loader": "^7.0.1",
        "prettier": "^2.7.1",
        "react-test-renderer": "^18.2.0",
        "tailwindcss": "^3.1.8",
        "ts-jest": "^29.0.3",
        "ts-loader": "^9.4.1",
        "typescript": "^4.8.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "webextension-polyfill": "^0.10.0"
    }
}
